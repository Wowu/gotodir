{
  "name": "gotodir",
  "version": "1.1.1",
  "description": "A cli app for fast directory changing.",
  "bin": {
    "gotodir": "./bin/gotodir",
    "gotodir-helper": "./bin/gotodir-helper"
  },
  "scripts": {
    "lint": "eslint src/ bin/gotodir-helper.js",
    "build":
      "babel -d lib/ src/ && babel -o bin/gotodir-helper bin/gotodir-helper.js && chmod +x bin/gotodir-helper",
    "prettier": "prettier --write src/**/*.js package.json bin/gotodir-helper.js"
  },
  "author": "Karol Musur",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "jsonfile": "^2.4.0",
    "left-pad": "^1.1.3",
    "lowdb": "^0.14.0",
    "omelette": "^0.3.1",
    "pryjs": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "prettier": "1.11.1"
  }
}
